{
  "version": 2,
  "buildCommand": "nuxt build",
  "outputDirectory": ".output/public",
  "devCommand": "nuxt dev",
  "framework": "nuxt",
  "ignoreCommand": "!.output",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NITRO_PRESET": "vercel"
  }
}
