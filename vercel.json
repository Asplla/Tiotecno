{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@vercel/node",
      "config": {
        "build": {
          "env": {
            "ROLLUP_SKIP_NODE_RESOLVE": "true"
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "ROLLUP_SKIP_NODE_RESOLVE": "true"
  }
} 